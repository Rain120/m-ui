{
  "name": "@zc/rmc-ui",
  "author": "Rain120",
  "version": "0.0.1",
  "description": "@react mobile components ui design for study",
  "main": "lib/rmc-ui.min.js",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "scripts": {
    "dev": "webpack-dev-server --color --config scripts/webpack.dev.config.js --progress --mode development --watch",
    "build:test": "NODE_ENV=development webpack --config webpack.config.js --progress",
    "build:dev": "NODE_ENV=development webpack --config scripts/webpack.dev.config.js --progress",
    "build:prod": "webpack --config scripts/webpack.prod.config.js --progress",
    "build:es": "tsc --project tsconfig.json --module ES6 --outDir es/",
    "lint": "tslint --type-check --project ./tsconfig.json",
    "build:analyze": "webpack --profile --json > ./analyze/stats.json --progress",
    "analyze": "webpack-bundle-analyzer ./analyze/stats.json",
    "precommit": "npm run lint",
    "docz:dev": "yarn docz dev",
    "docz:build": "yarn docz build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Rain120/rmc-ui.git"
  },
  "keywords": [
    "rmc-ui",
    "mobile-ui",
    "react.js",
    "react",
    "component",
    "components",
    "ui",
    "framework"
  ],
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "commit-msg": "commitlint -E $HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "last 4 version",
    "> 1%",
    "Firefox ESR",
    "ie >= 9",
    "iOS >= 8",
    "Android >= 4"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.1.0",
    "file-loader": "^4.1.0",
    "husky": "^3.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-import-plugin": "^1.6.1",
    "ts-loader": "^6.0.4",
    "tslint": "^5.16.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.36.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
