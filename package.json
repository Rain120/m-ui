{
  "name": "@zc/rmc-ui",
  "author": "Rain120",
  "version": "0.0.1",
  "description": "@react mobile components ui design for study",
  "license": "MIT",
  "main": "lib/rmc-ui.min.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Rain120/rmc-ui.git"
  },
  "keywords": [
    "rmc-ui",
    "mobile-ui",
    "react.js",
    "react",
    "component",
    "components",
    "ui",
    "framework"
  ],
  "scripts": {
    "clean:all": "rimraf dist lib es",
    "clean:lib/es": "rimraf lib es",
    "dev": "webpack-dev-server --color --config scripts/webpack.dev.config.js --progress --mode development --watch",
    "build:test": "cross-env NODE_ENV=development webpack --config webpack.config.js --progress",
    "build:dist": "cross-env NODE_ENV=development webpack --config scripts/webpack.dev.config.js --progress",
    "build:ts": "tsc --project ./tsconfig.json --declaration --module ES6 --outDir lib && cp -PR lib es",
    "build:lib/es": "npm run clean:lib/es && npm run build:ts && gulp",
    "build": "npm run clean:all && npm run build:dist && npm run build:ts && gulp",
    "build:analyze": "webpack --profile --json > ./analyze/stats.json --progress",
    "analyze": "webpack-bundle-analyzer ./analyze/stats.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint-staged": "lint-staged",
    "lint-staged:ts": "npm run tsc && node node_modules/tslint/bin/tslint -c ./tslint.json components/**/*.tsx",
    "lint": "tslint --type-check --project ./tsconfig.json && stylelint \"components/**/*.less\"",
    "precommit": "npm run lint",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "lint-staged": {
    "components/**/*.{tsx}": [
      "npm run lint-staged:ts"
    ],
    "{components}/**/*.less": "stylelint --syntax less"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ],
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "last 4 version",
    "> 1%",
    "Firefox ESR",
    "ie >= 9",
    "iOS >= 8",
    "Android >= 4"
  ],
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "conventional-changelog-cli": "^2.0.23",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "file-loader": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^3.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^9.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "ts-import-plugin": "^1.6.1",
    "ts-loader": "^6.0.4",
    "tslint": "^5.16.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.36.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
